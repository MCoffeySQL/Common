--Table
--create a landing table for extended events log capture
USE Common;

DROP TABLE IF EXISTS dbo.objectModificationLog;
GO

CREATE TABLE dbo.objectModificationLog(
  id INT IDENTITY(1,1) NOT NULL,
  modificationTime DATETIME NOT NULL,
  databaseID INT NULL,
  databaseName NVARCHAR(128) NULL,
  objectType NVARCHAR(128) NULL,
  objectID INT NULL,
  objectName NVARCHAR(128) NULL,
  sessionID INT NULL,
  loginName NVARCHAR(128) NULL,
  clientHostName NVARCHAR(128) NULL,
  clientAppName NVARCHAR(128) NULL,
  sqlStr NVARCHAR(MAX) NULL,
  phase NVARCHAR(128) NULL,
  dateCreated DATETIME NULL
);
GO
